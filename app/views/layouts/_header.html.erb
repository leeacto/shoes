<ul>
	<li><%= link_to 'Connect', users_path %></li>
	<% if current_user %>
		<li><%= link_to 'Log Out', logout_path %></li>
		<li><%= link_to 'My Account', user_path(current_user) %></li>
	<% else %>
		<li><%= link_to 'Log In', login_path %></li>
		<li><%= link_to 'Sign Up', new_user_path %></li>
	<% end %>
</ul>
